{
    "name": "rivalex/laravel-breadcrumbs",
    "description": "A simple Laravel-style way to create breadcrumbs with Font Awesome support.",
    "keywords": [
        "laravel"
    ],
    "homepage": "https://github.com/rivalex/laravel-breadcrumbs",
    "authors": [
        {
            "name": "Rivalex",
            "email": "info@rivalex.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "php-coveralls/php-coveralls": "^2.0",
        "spatie/phpunit-snapshot-assertions": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Rivalex\\Breadcrumbs\\": "classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/",
            "BreadcrumbsTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rivalex\\Breadcrumbs\\BreadcrumbsServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "Rivalex\\Breadcrumbs\\Facades\\Breadcrumbs"
            }
        }
    }
}
